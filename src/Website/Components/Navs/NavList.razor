@if (Items.Count() > 0 )
{
    @if (Type == NavType.ListGroup)
    {
        <ListGroupNav NavList="this" />
    } else if (Type == NavType.Tabs)
    {
        <TabsNav NavList="this" />
    }
    else
    {
        <PillsNav NavList="this" />
    }    
}


<CascadingValue Value="this" Name="NavList">
    @ChildContent
</CascadingValue>
